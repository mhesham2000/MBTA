{
  "name": "snowflake-sink",
  "config": {
    "connector.class": "com.snowflake.kafka.connector.SnowflakeSinkConnector",
    "tasks.max": "1",

    "topics.regex": "route_traffic",

    "snowflake.url.name": "UFJVTMA-EJA94314.snowflakecomputing.com",
    "snowflake.user.name": "POIUYTR",  
    "snowflake.private.key": "",
    "snowflake.database.name": "MBTA_DATA",
    "snowflake.schema.name": "PUBLIC",
    "snowflake.role.name": "ACCOUNTADMIN",
    "snowflake.warehouse.name": "COMPUTE_WH",



    "buffer.count.records": "10000",
    "buffer.size.bytes": "10000000",
    "buffer.flush.time": "60",

   
    "value.converter": "com.snowflake.kafka.connector.records.SnowflakeJsonConverter",
    "value.converter.schemas.enable": "false",


    "snowflake.metadata.topic2table.map": "route_traffic:ROUTE_TRAFFIC",

    
    "auto.create.tables": "true",  
    "errors.tolerance": "all"


  }
}